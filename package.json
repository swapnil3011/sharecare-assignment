{
  "name": "my-react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "preview": "vite preview",
    "lint:css": "stylelint \"**/*.{css,scss}\" --fix"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.59.0"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-clean-order"
    ],
    "ignoreFiles": ["dist/**/*"],
    "plugins": [
      "stylelint-order"
    ],
    "rules": {
      "order/order": [
        "custom-properties",
        "dollar-variables",
        "declarations",
        {
          "type": "at-rule",
          "name": "media"
        },
        "rules"
      ]
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "sass": "^1.89.2",
    "stylelint": "^16.21.0",
    "stylelint-config-clean-order": "^7.0.0",
    "vite": "^7.0.0"
  }
}
